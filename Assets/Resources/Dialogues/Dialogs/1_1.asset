  _dialogueLines:
  - participantTag: HeroTag
    text: "\u0412\u043E\u0442 \u044F \u0438 \u0434\u043E\u043C\u0430. \u0421\u043A\u043E\u043B\u044C\u043A\u043E
      \u0432\u0440\u0435\u043C\u0435\u043D\u0438 \u043F\u0440\u043E\u0448\u043B\u043E
      \u0441 \u0442\u0435\u0445 \u043F\u043E\u0440, \u043A\u0430\u043A \u044F \u043F\u043E\u043A\u0438\u043D\u0443\u043B
      \u044D\u0442\u0438 \u043C\u0435\u0441\u0442\u0430."
  - participantTag: HeroTag
    text: "\u041A\u0430\u0436\u0435\u0442\u0441\u044F, \u0431\u0443\u0434\u0442\u043E
      \u0432\u0447\u0435\u0440\u0430 \u044F \u043F\u043E\u043F\u0440\u043E\u0449\u0430\u043B\u0441\u044F
      \u0441 \u0443\u0447\u0438\u0442\u0435\u043B\u0435\u043C, \u0443\u0432\u0435\u0440\u0435\u043D\u043D\u044B\u0439
      \u0432 \u0441\u0432\u043E\u0435\u0439 \u043C\u0438\u0441\u0441\u0438\u0438.
      \u041D\u043E \u0441\u0435\u0439\u0447\u0430\u0441 \u044F \u0447\u0443\u0432\u0441\u0442\u0432\u0443\u044E,
      \u043A\u0430\u043A \u043C\u043D\u043E\u0433\u043E \u0438\u0437\u043C\u0435\u043D\u0438\u043B\u043E\u0441\u044C
      \u0432\u043E \u043C\u043D\u0435"
  - participantTag: HeroTag
    text: "\u041C\u043E\u0439 \u0443\u0447\u0438\u0442\u0435\u043B\u044C \u0432\u0441\u0435\u0433\u0434\u0430
      \u0437\u043D\u0430\u043B, \u043A\u0430\u043A \u043D\u0430\u043F\u0440\u0430\u0432\u0438\u0442\u044C
      \u043C\u0435\u043D\u044F \u0438 \u0434\u0430\u0442\u044C \u0432\u0435\u0440\u0443.
      \u041D\u043E \u0442\u0435\u043F\u0435\u0440\u044C \u044F \u043E\u0449\u0443\u0449\u0430\u044E
      \u0442\u0440\u0435\u0432\u043E\u0433\u0443, \u0434\u0443\u043C\u0430\u044F,
      \u0447\u0442\u043E \u043C\u043D\u043E\u0433\u043E\u0435 \u0431\u044B\u043B\u043E
      \u0441\u043A\u0440\u044B\u0442\u043E. "
  - participantTag: HeroTag
    text: "\u041F\u043E\u0440\u0430 \u043D\u0430\u0439\u0442\u0438 \u0435\u0433\u043E
      \u0438 \u0443\u0437\u043D\u0430\u0442\u044C, \u0447\u0442\u043E \u0431\u0443\u0434\u0435\u0442
      \u0434\u0430\u043B\u044C\u0448\u0435. \u041D\u0430\u0434\u0435\u044E\u0441\u044C,
      \u043E\u043D \u043E\u0442\u0432\u0435\u0442\u0438\u0442 \u043D\u0430 \u0432\u0441\u0435
      \u043C\u043E\u0438 \u0432\u043E\u043F\u0440\u043E\u0441\u044B."
  - participantTag: HeroTag
    text: "\u041D\u0430\u0432\u0435\u0440\u043D\u043E\u0435, \u0443\u0447\u0438\u0442\u0435\u043B\u044C
      \u043D\u0430\u0445\u043E\u0434\u0438\u0442\u0441\u044F \u043D\u0430 \u0434\u0435\u0440\u0435\u0432\u0435\u043D\u0441\u043A\u043E\u0439
      \u043F\u043B\u043E\u0449\u0430\u0434\u0438. \u041F\u043E\u043F\u0440\u043E\u0431\u0443\u044E
      \u0440\u0430\u0437\u044B\u0441\u043A\u0430\u0442\u044C \u0435\u0433\u043E \u0442\u0430\u043C."
  - participantTag: FirstSceneMonolog
    text: ' '
